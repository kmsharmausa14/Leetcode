@charset "utf-8";

.tags_table td, .tags_table th { padding-left: 2px; padding-right: 2px; }

.tags_table_column_tag_caption { text-align: left; }

.tags_table_column_count_caption { text-align: right; }

.tags_table tfoot, .tags_table thead { color: rgb(255, 255, 255); background: rgb(0, 0, 0); }

.tags_table th { cursor: pointer; padding-top: 2px; padding-bottom: 2px; }

.tags_table th:hover { text-decoration: underline; }

.tags_table th::after, .tags_table th[sortdir="down"]::after { content: " ↑"; }

.tags_table th[sortdir="up"]::after { content: " ↓"; }

.tags_table tbody tr:hover { background: lightgrey; }

.tags_table_column_count { text-align: right; }

a.all-tags-link { display: block; font-size: small; text-align: center; margin: 0.5em auto auto; }

.tags-page-pagination { margin-bottom: 0.6em; }

.tags-page-pagination a, .tags-page-pagination span { color: gray; border: 1px solid gray; padding-left: 0.3em; padding-right: 0.3em; margin-left: 0.1em; margin-right: 0.1em; }

.tags-page-pagination span:first-child { margin-left: 0px; margin-right: 0.3em; }

.tags-page-pagination span.current-page { color: rgb(0, 0, 0); border: 1px solid rgb(0, 0, 0); }
